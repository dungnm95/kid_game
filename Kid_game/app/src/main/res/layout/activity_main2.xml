<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context="com.k63b.kid_game.Main2Activity"
    android:background="@drawable/bg">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/back"
        android:id="@+id/btn_back"
        android:layout_marginRight="538dp"
        android:layout_marginEnd="538dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="340dp"
        android:onClick=""
        android:layout_marginLeft="0dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/thong_bao"
        android:id="@+id/noti"
        android:paddingLeft="350dp"
        android:layout_alignBottom="@+id/heart"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="0dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/cau_hoi"
        android:id="@+id/imageView4"
        android:layout_marginTop="170dp"
        android:layout_below="@+id/noti"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="500dp"
        android:layout_marginRight="0dp"
        android:layout_marginLeft="500dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/heart"
        android:id="@+id/heart"
        android:paddingLeft="50dp"
        android:paddingRight="0dp"
        android:layout_marginBottom="336dp"
        android:elevation="2dp"
        android:paddingBottom="5dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="520dp"
        android:paddingTop="5dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/cau_hoi_so_6"
        android:id="@+id/cau_hoi"
        android:layout_below="@+id/noti"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="25dp"
        android:layout_marginStart="25dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/so_3"
        android:id="@+id/so_3"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/so_9"
        android:layout_alignStart="@+id/so_9"
        android:layout_marginBottom="50dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/so_8"
        android:id="@+id/so_8"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/so_9"
        android:layout_toEndOf="@+id/so_9" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/so_2"
        android:id="@+id/so_2"
        android:layout_below="@+id/cau_hoi"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/so_7"
        android:id="@+id/so_7"
        android:layout_below="@+id/cau_hoi"
        android:layout_toLeftOf="@+id/so_5"
        android:layout_toStartOf="@+id/so_5" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/so_9"
        android:id="@+id/so_9"
        android:layout_above="@+id/so_3"
        android:layout_alignLeft="@+id/cau_hoi"
        android:layout_alignStart="@+id/cau_hoi"
        android:layout_marginBottom="30dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/so_10"
        android:id="@+id/so_10"
        android:layout_marginRight="45dp"
        android:layout_marginEnd="45dp"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/so_2"
        android:layout_alignEnd="@+id/so_2" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/so_1"
        android:id="@+id/so_1"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/so_6"
        android:layout_toStartOf="@+id/so_6"
        android:layout_marginLeft="-10dp"
        android:layout_marginRight="190dp"
        android:layout_marginBottom="20dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/so_5"
        android:id="@+id/so_5"
        android:elevation="1dp"
        android:layout_alignTop="@+id/so_9"
        android:layout_alignLeft="@+id/imageView4"
        android:layout_alignStart="@+id/imageView4" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/so_6"
        android:id="@+id/so_6"
        android:layout_below="@+id/so_8"
        android:layout_alignLeft="@+id/so_7"
        android:layout_alignStart="@+id/so_7" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/so_4"
        android:id="@+id/so_4"
        android:layout_alignTop="@+id/imageView4"
        android:layout_toLeftOf="@+id/so_6"
        android:layout_toStartOf="@+id/so_6" />


</RelativeLayout>
