<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context="com.k63b.kid_game.Main5Activity"
    android:background="@drawable/bg">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/back"
        android:id="@+id/btn_back"
        android:layout_marginRight="538dp"
        android:layout_marginEnd="538dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="340dp"
        android:onClick=""
        android:layout_marginLeft="0dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/heart"
        android:id="@+id/heart"
        android:layout_marginLeft="291dp"
        android:layout_marginStart="291dp"
        android:layout_alignBottom="@+id/noti"
        android:layout_alignLeft="@+id/noti"
        android:layout_alignStart="@+id/noti"
        android:layout_marginBottom="11dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/thong_bao"
        android:layout_marginRight="34dp"
        android:layout_marginEnd="34dp"
        android:id="@+id/noti"
        android:layout_marginBottom="319dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/cau_hoi_so_1"
        android:id="@+id/cau_hoi"
        android:layout_marginTop="55dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/so_3"
        android:id="@+id/so_3"
        android:layout_above="@+id/so_2"
        android:layout_alignRight="@+id/cau_hoi"
        android:layout_alignEnd="@+id/cau_hoi"
        android:layout_marginRight="14dp"
        android:layout_marginEnd="14dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/so_1"
        android:id="@+id/so_1"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/noti"
        android:layout_toStartOf="@+id/noti"
        android:layout_marginRight="63dp"
        android:layout_marginEnd="63dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/cau_hoi"
        android:id="@+id/cat"
        android:layout_marginLeft="78dp"
        android:layout_marginStart="78dp"
        android:layout_below="@+id/so_3"
        android:layout_alignLeft="@+id/so_3"
        android:layout_alignStart="@+id/so_3" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/so_2"
        android:id="@+id/so_2"
        android:layout_below="@+id/so_1"
        android:layout_toLeftOf="@+id/so_3"
        android:layout_toStartOf="@+id/so_3"
        android:layout_marginRight="36dp"
        android:layout_marginEnd="36dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="@android:dimen/dialog_min_width_major"
        app:srcCompat="@drawable/noti_0"
        android:layout_alignRight="@+id/noti"
        android:layout_alignEnd="@+id/noti"
        android:layout_marginRight="17dp"
        android:layout_marginEnd="17dp"
        android:id="@+id/noti_2"
        android:layout_alignParentTop="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="@android:dimen/dialog_min_width_major"
        app:srcCompat="@drawable/noti_trong"
        android:id="@+id/noti_1"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/noti_2"
        android:layout_toStartOf="@+id/noti_2" />


</RelativeLayout>
